{
  "jest.debugCodeLens.showWhenTestStateIn": ["skip", "pass", "fail", "unknown"],
  "cSpell.words": [
    
  ],

  "files.exclude": {
    // Hide the node_modules folder
    "node_modules/": true,
    // Hide all files that end in .d.ts
    "**/*.d.ts": true,
    // Hide all files that end with .js if they have a .ts file
    "**/*.js": {
      "when": "$(basename).ts"
    },
  }
}
